{
  "name": "remote-data-js",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "watch": {
    "test-no-fail": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js",
      "quiet": "true"
    }
  },
  "scripts": {
    "build-umd": "webpack --config webpack/webpack.umd.config.js",
    "build-cjs": "babel src -d lib",
    "build-all": "npm run build-umd && npm run build-cjs",
    "prepublish": "npm run build-all",
    "only-tests": "tape -r babel-register test/*-test.js | tap-min",
    "test": "npm run lint && npm run only-tests",
    "test-no-fail": "npm only-tests || true",
    "watch": "npm-watch",
    "lint": "eslint src/*.js test/*.js"
  },
  "keywords": [],
  "author": "Jack Franklin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "^6.6.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "6.9.0",
    "doubler": "0.6.0",
    "eslint": "2.13.1",
    "eslint-config-standard": "5.3.1",
    "eslint-plugin-promise": "1.3.2",
    "eslint-plugin-standard": "1.3.2",
    "fetch-mock": "4.5.4",
    "npm-watch": "0.1.4",
    "tap-min": "1.1.0",
    "tape": "4.5.1",
    "tape-promise": "1.1.0",
    "webpack": "^1.12.14",
    "webpack-notifier": "1.3.0"
  }
}
